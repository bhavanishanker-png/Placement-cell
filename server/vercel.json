{
  "version": 2,
  "builds": [
    {
      "src": "server/api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/server.js"
    }
  ],
  "env": {
    "DB_HOST": "@db-host",
    "DB_USER": "@db-user",
    "DB_PASSWORD": "@db-password",
    "DB_NAME": "@db-name",
    "DB_PORT": "@db-port",
    "JWT_SECRET": "@jwt-secret",
    "REACT_APP_BACKEND_URL": "@react-app-backend-url"
  },
  "functions": {
    "server/api/server.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
